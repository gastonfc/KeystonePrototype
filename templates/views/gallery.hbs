<div class="container">
	<h1>Gallery</h1>
</div>
<div class="container">
	<!-- if galleries -->
	{{#if galleries}}
		{{#each galleries}}
		<h2>{{name}}
			<div class="pull-right text-muted">19th May 2014</div>
		</h2>
    {{> gallery}}
		{{/each}}
	{{else}}
	<!-- else -->
	<h3 class="text-muted">There are no image galleries yet.</h3>
	{{/if}}
</div>
